                        <div class="span10">

                          <%= text_field_tag 'app_key', '', placeholder: 'Input your API key! (ex: 57d77eda)' %>
                        </div>
                      </li>

                      <li class="clearfix summary-spacer">
                        <h3 class="span4 property">Token<br></h3>
                        <div class="span20">
                          <div class="span12 inner-left">

                            <%= text_field_tag 'app_token', '', placeholder: 'Input your Token! (ex: eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ2ZXJzaW...)' %>
                          </div>
