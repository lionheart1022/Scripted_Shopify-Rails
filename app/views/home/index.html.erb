              <script type="text/javascript">
                  (function($) {
                      $('#app-secret-show').click(function() {
                          $('.secrets-list').addClass('show');
                          $('#app-secret-show').css('display', 'none');
                          return false;
                      });
                  })(jQuery);
              </script>
            </div>
            <script type="text/javascript">
                (function($) {
                    function displayNoReviewsYet() {
                        $(".reviews-progress").each(function() {
                            $(this).text("No reviews yet");
                        });
                    }

                    $(document).ready(function() {
                        var url = "\/\/apps.shopify.com\/c3d2dba94ec3418290391d03c77b1bae\/reviews.json";

                        $.ajax({
                            type: 'GET',
                            url: url,
                            dataType: 'json',
                            success: function(json) {
                                var rating = $("#rating-star-overall");

                                rating.find(".rating-star-halves").addClass('rating-star-halves-' + json.overall_rating);
                                rating.show();
                                $(".reviews-progress").hide();

                                var len = json.reviews.length;

                                if(len == 0) {
                                    displayNoReviewsYet();
                                    return;
                                }

                                if(len >= 3) {
                                    len = 3;
                                }

                                for (var i = 0; i < len; i++) {
                                    var review = json.reviews[i],
                                            clone = $("#reviews").children(".review").first().clone();

                                    clone.find(".rating-star-halves").addClass("rating-star-" + review.star_rating);
                                    clone.find(".review-content").text(review.body);
                                    if (review.shop_domain)
                                        clone.find(".shop-link").attr("href", "http://" + review.shop_domain).text(review.shop_name);
                                    else
                                        clone.find(".shop-link").replaceWith($("<span />", { text: review.shop_name }));


                                    clone.appendTo("#reviews");
                                }

                                $("#reviews").children(".review").first().detach();
                                $("#reviews").show();
                            },
                            error: function() {
                                displayNoReviewsYet();
                            }
                        });
                    })
                })(jQuery);
            </script>
          </div>
        </div>
